{
  "name": "jbeat-ui-suite",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.15.6",
  "scripts": {
    "dev:uikit": "turbo run storybook --filter=@jbeat/uikit",
    "dev:pages": "turbo run dev --filter=@jbeat/pages",
    "build": "turbo run format && turbo run lint && turbo run test && turbo run build",
    "build:uikit": "turbo run format --filter=@jbeat/uikit && turbo run lint --filter=@jbeat/uikit && turbo run test --filter=@jbeat/uikit && turbo run build --filter=@jbeat/uikit",
    "build:pages": "turbo run format --filter=@jbeat/pages && turbo run lint --filter=@jbeat/pages && turbo run test --filter=@jbeat/pages && turbo run build --filter=@jbeat/pages",
    "format": "turbo run format",
    "lint": "turbo run lint",
    "lint:uikit": "turbo run lint --filter=@jbeat/uikit",
    "lint:pages": "turbo run lint --filter=@jbeat/pages",
    "test": "turbo run test",
    "test:uikit": "turbo run test --filter=@jbeat/uikit",
    "test:pages": "turbo run test --filter=@jbeat/pages",
    "test:e2e": "turbo run test:e2e",
    "test:uikit:e2e": "turbo run test:e2e --filter=@jbeat/uikit",
    "test:pages:e2e": "turbo run test:e2e --filter=@jbeat/pages",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish",
    "release:dev": "pnpm build && changeset publish --tag next"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.7",
    "@playwright/test": "1.56.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.19.1",
    "@types/react": "19.2.5",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.46.4",
    "@typescript-eslint/parser": "8.46.4",
    "@vitejs/plugin-react-swc": "3.11.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "9.1.2",
    "jest": "29.7.0",
    "playwright": "1.56.1",
    "prettier": "3.6.2",
    "ts-jest": "29.4.5",
    "turbo": "2.6.1",
    "typescript": "5.9.3",
    "vite": "6.4.1",
    "vitest": "2.1.9"
  }
}
